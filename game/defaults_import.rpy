# Icon calls

label Good:
    if evilness <= 0:
        $ evilness = 0
        return
    $ evilness -= 1
    window hide
    play sound "sounds/skill.mp3"
    show superhero at poslocation
    $ renpy.pause(0.5, hard=True)
    return

label Evil:
    if evilness >= 22:
        $ evilness = 22
        return
    $ evilness += 1
    window hide
    play sound "sounds/skill.mp3"
    show supervillain at poslocation
    $ renpy.pause(0.5, hard=True)
    return

